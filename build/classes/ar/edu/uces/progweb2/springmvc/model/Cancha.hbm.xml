<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ar.edu.uces.progweb2.springmvc.model.Cancha" table="CANCHAS">
		<id column="ID_CANCHA" name="id">
			<generator class="native" />
		</id>
		<many-to-one name="sede" column="ID_SEDE" lazy="false" />
		<property column="NUMERO" name="numero" />
		<property column="TIPO" name="tipo" />
	</class>
</hibernate-mapping>